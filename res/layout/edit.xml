    <EditText
        android:id="@+id/et44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et33"
        android:layout_below="@+id/et33"
        android:layout_marginTop="38dp"
        android:ems="10" />

    <TextView
        android:id="@+id/etv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et11"
        android:layout_alignParentTop="true"
        android:text="name" />

    <TextView
        android:id="@+id/et3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et22"
        android:layout_below="@+id/et22"
