    <TextView
        android:id="@+id/et5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et44"
        android:layout_below="@+id/et44"
        android:text="phon" />

    <EditText
        android:id="@+id/et33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et3"
        android:layout_below="@+id/et3"
        android:layout_marginTop="23dp"
        android:ems="10" />

    <EditText
        android:id="@+id/et55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et5"
        android:layout_below="@+id/et5"
        android:layout_marginTop="15dp"
        android:ems="10" />

</RelativeLayout>
